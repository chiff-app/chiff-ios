{
    "characterSets": [{
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyz",
            "name": "LowerLetters"
        },
        {
            "base": [],
            "characters": "ABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "UpperLetters"
        },
        {
            "base": [],
            "characters": "1234567890",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~| -_+=",
            "name": "Specials"
        },
        {
            "base": [],
            "characters": " ",
            "name": "Spaces"
        }
    ],
    "properties": {
        "characterSettings": null,
        "maxConsecutive": null,
        "minLength": 8,
        "maxLength": 32,
        "expires": null
    },
    "version": null,
    "timestamp": null,
    "url": "https://www.netflix.com",
    "redirect": null,
    "name": "Netflix",
    "service": {
        "login": {
            "url": "https://www.netflix.com",
            "maxTries": 3,
            "routines": [{
                "type": "htmlLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.netflix.com/Login",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.netflix.com/Login"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#appMountPoint > div > div.login-body > div > div > form:nth-child(2)"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#email"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{password}}"
                            }
                        ],
                        "assert": {
                            "assertions": [{
                                "selector": ".profile-button",
                                "minOccurs": 1,
                                "maxOccurs": null
                            }]
                        }
                    }
                ]
            }]
        },
        "register": {
            "url": "https://www.netflix.com"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "extendedJSPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.netflix.com/password",
                        "assert": {
                            "assertions": [{
                                "expression": null,
                                "prefix": "https://www.netflix.com/password"
                            }]
                        }
                    },
                    {
                        "type": "javascript",
                        "script": "var inputEvent = new Event('input', { bubbles: true }); inputEvent.simulated = true; document.querySelector('input#password').value = '{{password}}'; document.querySelector('input#password').dispatchEvent(inputEvent); document.querySelector('input#pw_new').value = '{{newPassword}}'; document.querySelector('input#pw_new').dispatchEvent(inputEvent); document.querySelector('input#pw_confirm').value = '{{newPassword}}'; document.querySelector('input#pw_confirm').dispatchEvent(inputEvent); document.querySelector('button[type=submit]').click();"
                    }
                ],
                "login": "htmlLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://www.netflix.com/nl/loginhelp",
            "maxTries": null,
            "routines": null
        }
    }
}
