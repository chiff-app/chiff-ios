        "login": {
            "url": "https://www.marktplaats.nl/account/login.html",
            "maxTries": 3,
            "routines": [{
                "type": "htmlLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.marktplaats.nl/account/login.html",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.marktplaats.nl/account/login.html"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#account-login-form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#account-login-form > fieldset > div > div:nth-child(1) > input"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{password}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "select",
                                "selector": "a[data-role=mymp-username]",
                                "minOccurs": 1,
                                "maxOccurs": 1
                            }
                        ]
                    }
                ]
            }]
        },
        "register": {
            "url": "https://www.marktplaats.nl/account/createAccount.html"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "htmlPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.marktplaats.nl/account/wachtwoordinstellen.html",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.marktplaats.nl/account/wachtwoordinstellen.html"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#new-password-form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#new-password"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#passwordConfirm"
                                },
                                "value": "{{newPassword}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.marktplaats.nl/account/login.html"
                            }
                        ]
                    }
                ],
