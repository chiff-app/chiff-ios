{
    "characterSets": [{
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "Letters"
        },
        {
            "base": [],
            "characters": "0123456789",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~| -",
            "name": "Specials"
        }
    ],
    "properties": {
        "characterSettings": null,
        "maxConsecutive": null,
        "minLength": 8,
        "maxLength": null,
        "expires": null
    },
    "version": null,
    "timestamp": null,
    "url": "https://www.facebook.com/",
    "redirect": null,
    "name": "Facebook",
    "service": {
        "login": {
            "url": "https://www.facebook.com/",
            "maxTries": 3,
            "routines": [{
                    "type": "htmlLoginRoutine",
                    "instructions": [{
                            "type": "get",
                            "url": "https://www.facebook.com/",
                            "assert": {
                                "assertions": [{
                                    "expression": null,
                                    "prefix": "https://www.facebook.com"
                                }]
                            }
                        },
                        {
                            "type": "form",
                            "selector": {
                                "value": "#login_form"
                            },
                            "element": [{
                                    "selector": {
                                        "value": "#email"
                                    },
                                    "value": "{{username}}"
                                },
                                {
                                    "selector": {
                                        "value": "#pass"
                                    },
                                    "value": "{{password}}"
                                }
                            ],
                            "assert": {
                                "assertions": [{
                                    "selector": "#userMenu",
                                    "minOccurs": 1,
                                    "maxOccurs": 1
                                }]
                            }
                        }
                    ]
                },
                {
                    "type": "htmlLoginRoutine",
                    "instructions": [{
                            "type": "get",
                            "url": "https://www.facebook.com/login.php",
                            "assert": {
                                "assertions": [{
                                    "expression": null,
                                    "prefix": "https://www.facebook.com"
                                }]
                            }
                        },
                        {
                            "type": "form",
                            "selector": {
                                "value": "#login_form"
                            },
                            "element": [{
                                    "selector": {
                                        "value": "#email"
                                    },
                                    "value": "{{username}}"
                                },
                                {
                                    "selector": {
                                        "value": "#pass"
                                    },
                                    "value": "{{password}}"
                                }
                            ],
                            "assert": {
                                "assertions": [{
                                    "selector": "#userMenu",
                                    "minOccurs": 1,
                                    "maxOccurs": 1
                                }]
                            }
                        }
                    ]
                }
            ]
        },
        "register": {
            "url": "https://www.facebook.com/"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "extendedJSPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.facebook.com/settings?tab=security&section=password&view",
                        "assert": {
                            "assertions": [{
                                "expression": null,
                                "prefix": "https://www.facebook.com/settings?tab=security&section=password&view"
                            }]
                        }
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#password > div > div > form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#password_old"
                                },
                                "value": "{{password}}"
                            },
                            {
                                "selector": {
                                    "value": "#password_new"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#password_confirm"
                                },
                                "value": "{{newPassword}}"
                            }
                        ],
                        "assert": {
                            "assertions": [{
                                "selector": "div.success",
                                "minOccurs": 1,
                                "maxOccurs": 1
                            }]
                        }
                    },
                    {
                        "type": "javascript",
                        "script": "document.querySelector('input#password_confirm').click(); document.querySelector('#password > div > div > form [type=submit]').click();"
                    }
                ],
                "login": "htmlLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://www.facebook.com/login/identify",
            "maxTries": null,
            "routines": null
        }
    }
}
