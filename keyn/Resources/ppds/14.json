{
    "characterSets": [
        {
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "Letters"
        },
        {
            "base": [],
            "characters": "0123456789",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~| -_+=",
            "name": "Specials"
        },
        {
            "base": [],
            "characters": " ",
            "name": "Spaces"
        }
    ],
    "properties": {
        "minLength": 8,
        "expires": 0,
        "characterSettings": {
            "characterSetSettings": [
                {
                    "name": "Letters",
                    "minOccurs": 1
                }
            ]
        },
        "maxConsecutive": 3,
        "maxLength": 15
    },
    "timestamp": null,
    "url": "https://www.mediamarkt.nl",
    "name": "Mediamarkt",
    "version": "1.0",
    "service": {
        "login": {
            "url": "https://www.mediamarkt.nl/",
            "maxTries": 3,
            "routines": [{
                "type": "htmlLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/LogonForm",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/LogonForm"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#login-form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#login-email"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#login-password"
                                },
                                "value": "{{password}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/MultiChannelMAShop"
                            }
                        ]
                    }
                ]
            }]
        },
        "register": {
            "url": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/MultiChannelMARegister"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "htmlPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/MultiChannelMAChangePassword",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/MultiChannelMAChangePassword"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#change-password-form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#current-password"
                                },
                                "value": "{{password}}"
                            },
                            {
                                "selector": {
                                    "value": "#new-password"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#new-password-confirm"
                                },
                                "value": "{{newPassword}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.mediamarkt.nl/webapp/wcs/stores/servlet/MultiChannelMAPersonal"
                            }
                        ]
                    }
                ],
                "login": "htmlLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://www.coolblue.nl/wachtwoordvergeten",
            "maxTries": null,
            "routines": null
        }
    }
}
