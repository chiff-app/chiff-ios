{
    "characterSets": [{
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyz",
            "name": "LowerLetters"
        },
        {
            "base": [],
            "characters": "ABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "UpperLetters"
        },
        {
            "base": [],
            "characters": "1234567890",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~|",
            "name": "Specials"
        },
        {
            "base": [],
            "characters": " ",
            "name": "Spaces"
        }
    ],
    "properties": {
        "characterSettings": null,
        "maxConsecutive": null,
        "minLength": 6,
        "maxLength": null,
        "expires": null
    },
    "version": null,
    "timestamp": null,
    "url": "https://datumprikker.nl/",
    "redirect": null,
    "name": "Datumprikker",
    "service": {
        "login": {
            "url": "https://datumprikker.nl/account/login",
            "maxTries": 3,
            "routines": [{
                "type": "extendedJSLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://datumprikker.nl/account/login",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://datumprikker.nl/account/login"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#loginform"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#email"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{password}}"
                            }
                        ]
                    },
                    {
                        "type": "javascript",
                        "script": "document.querySelector(\"#login_submit\").click()"
                    }
                ]
            }]
        },
        "register": {
            "url": "https://datumprikker.nl/account/aanmaken"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "extendedJSPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://datumprikker.nl/account",
                        "assert": [
                            {
                                "type":"url",
                                "expression": null,
                                "prefix": "https://datumprikker.nl/account"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#loginform"
                        },
                        "element": [
                            {
                                "selector": {
                                    "value": "#oldpassword"
                                },
                                "value": "{{password}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#repeatPassword"
                                },
                                "value": "{{newPassword}}"
                            }
                        ]
                    },
                    {
                        "type": "javascript",
                        "script": "document.querySelector(\"#save-password\").click()",
                        "assert": [
                            {
                                "type": "select",
                                "selector": "TODO",
                                "minOccurs": 1,
                                "maxOccurs": null
                            }
                        ]
                    }
                ],
                "login": "extendedJSLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://datumprikker.nl/account/nieuwwachtwoordinstellen",
            "maxTries": null,
            "routines": null
        }
    }
}
