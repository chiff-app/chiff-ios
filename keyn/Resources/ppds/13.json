{
    "characterSets": [{
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyz",
            "name": "LowerLetters"
        },
        {
            "base": [],
            "characters": "ABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "UpperLetters"
        },
        {
            "base": [],
            "characters": "1234567890",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~|+=",
            "name": "Specials"
        }
    ],
    "properties": {
        "characterSettings": null,
        "maxConsecutive": null,
        "minLength": 6,
        "maxLength": null,
        "expires": null
    },
    "version": null,
    "timestamp": null,
    "url": "https://datumprikker.nl/",
    "redirect": null,
    "name": "Datumprikker",
    "service": {
        "login": {
            "url": "https://datumprikker.nl/account/login",
            "maxTries": 3,
            "routines": [{
                "type": "htmlLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://datumprikker.nl/account/login",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://datumprikker.nl/account/login"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#loginform"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#email"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{password}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.airbnb.com/dashboard"
                            }
                        ]
                    }
                ]
            }]
        },
        "register": {
            "url": "https://datumprikker.nl/account/aanmaken"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "htmlPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.airbnb.com/users/security/183519205",
                        "assert": [
                            {
                                "type":"url",
                                "expression": null,
                                "prefix": "https://www.airbnb.com/users/security/183519205"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#site-content > div > div > div.col-md-9 > form"
                        },
                        "element": [
                            {
                                "selector": {
                                    "value": "#old_password"
                                },
                                "value": "{{password}}"
                            },
                            {
                                "selector": {
                                    "value": "#new_password"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#user_password_confirmation"
                                },
                                "value": "{{newPassword}}"
                            }
                        ],
                        "assert": [
                            {
                                "selector": "TODO",
                                "minOccurs": 1,
                                "maxOccurs": 1
                            }
                        ]
                    }
                ],
                "login": "htmlLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://datumprikker.nl/account/nieuwwachtwoordinstellen",
            "maxTries": null,
            "routines": null
        }
    }
}
