{
    "characterSets": [
        {
            "base": [],
            "characters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",
            "name": "Letters"
        },
        {
            "base": [],
            "characters": "0123456789",
            "name": "Numbers"
        },
        {
            "base": [],
            "characters": ")(*&^%$#@!{}[]:;\"'?/,.<>`~| -_+=",
            "name": "Specials"
        },
        {
            "base": [],
            "characters": " ",
            "name": "Spaces"
        }
    ],
    "properties": {
        "minLength": 6,
        "expires": 0,
        "characterSettings": null,
        "maxConsecutive": null,
        "maxLength": null
    },
    "timestamp": null,
    "url": "https://www.strava.com/",
    "name": "Strava",
    "version": "1.0",
    "service": {
        "login": {
            "url": "https://www.strava.com/login",
            "maxTries": 3,
            "routines": [{
                "type": "htmlLoginRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.strava.com/login",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.strava.com/login"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "#login_form"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#email"
                                },
                                "value": "{{username}}"
                            },
                            {
                                "selector": {
                                    "value": "#password"
                                },
                                "value": "{{password}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "select",
                                "selector": "a[href$=session]",
                                "minOccurs": 1,
                                "maxOccurs": null
                            }
                        ]
                    }
                ]
            }]
        },
        "register": {
            "url": "https://www.strava.com/register/free"
        },
        "passwordChange": {
            "url": null,
            "maxTries": null,
            "routines": [{
                "type": "htmlPasswordChangeRoutine",
                "instructions": [{
                        "type": "get",
                        "url": "https://www.strava.com/settings/password",
                        "assert": [
                            {
                                "type": "url",
                                "expression": null,
                                "prefix": "https://www.strava.com/settings/password"
                            }
                        ]
                    },
                    {
                        "type": "form",
                        "selector": {
                            "value": "form[action$=update_email_password]"
                        },
                        "element": [{
                                "selector": {
                                    "value": "#athlete_old_password"
                                },
                                "value": "{{password}}"
                            },
                            {
                                "selector": {
                                    "value": "#athlete_password"
                                },
                                "value": "{{newPassword}}"
                            },
                            {
                                "selector": {
                                    "value": "#athlete_password_confirmation"
                                },
                                "value": "{{newPassword}}"
                            }
                        ],
                        "assert": [
                            {
                                "type": "select",
                                "selector": "div.alert.success",
                                "minOccurs": 1,
                                "maxOccurs": null
                            }
                        ]
                    }
                ],
                "login": "htmlLoginRoutine"
            }]
        },
        "passwordReset": {
            "url": "https://www.strava.com/account/recover",
            "maxTries": null,
            "routines": null
        }
    }
}
